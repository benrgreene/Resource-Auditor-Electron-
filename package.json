{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "vue-cli-service build",
    "watch": "vue-cli-service build --watch"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron"
  ],
  "author": "Ben Greene",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "devDependencies": {
    "core-js": "^3.3.2",
    "electron": "^15.5.5",
    "vue": "^2.6.10",
    "vuex": "^3.1.2"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "electron-log": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-system": "^2.2.2",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "process": "^0.11.10",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.43.0",
    "webpagetest": "^0.3.9"
  }
}
